    </LevelOfAttackProbability>
    <!--  Средний -->
    <LevelOfAttackProbability>
    	<id>3</id>
        <Name>Средний</Name>
        <Description>Вероятность проведения атаки приблизительно равна 0,5.</Description>
    </LevelOfAttackProbability>
    <!--  Высокий -->
    <LevelOfAttackProbability>
    	<id>4</id>
        <Name>Высокий</Name>
        <Description>Атака скорее всего будет проведена.</Description>
    </LevelOfAttackProbability>
    <!--  Очень высокий -->
    <LevelOfAttackProbability>
    	<id>5</id>
        <Name>Очень высокий</Name>
        <Description>Атака почти наверняка будет проведена.</Description>
    </LevelOfAttackProbability>    
</LevelsOfAttackProbability>
